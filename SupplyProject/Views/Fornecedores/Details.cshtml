@model SupplyProject.Models.Fornecedor

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>Fornecedor</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Id Fornecedor
        </dt>

        <dd>
            @Html.DisplayFor(model => model.idFornecedor)
        </dd>

        <dt>
            Nome
        </dt>

        <dd>
            @Html.DisplayFor(model => model.nome_fornecedor)
        </dd>

        <dt>
           CNPJ
        </dt>

        <dd>
            @Html.DisplayFor(model => model.cnpj_fornecedor)
        </dd>

        <dt>
            Endereco
        </dt>

        <dd>
            @Html.DisplayFor(model => model.logradouro_fornecedor), @Html.DisplayFor(model => model.numlogradouro_fornecedor)
        </dd>

        <dt>
            E-mail
        </dt>

        <dd>
            @Html.DisplayFor(model => model.email_fornecedor)
        </dd>

        <dt>
            Telefone
        </dt>

        <dd>
            @Html.DisplayFor(model => model.telefone_fornecedor)
        </dd>

    </dl>
</div>
<p>
    @Html.ActionLink("Voltar", "Index")
</p>
