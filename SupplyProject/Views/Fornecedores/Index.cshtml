@model IEnumerable<SupplyProject.Models.Fornecedor>

@{
    ViewBag.Title = "Index";
}

@{
    Layout = "~/Views/Shared/_LayoutIndex.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.nome_fornecedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.cnpj_fornecedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.logradouro_fornecedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.numlogradouro_fornecedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.telefone_fornecedor)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.email_fornecedor)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nome_fornecedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cnpj_fornecedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.logradouro_fornecedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.numlogradouro_fornecedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.telefone_fornecedor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.email_fornecedor)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.idFornecedor }) |
                @Html.ActionLink("Details", "Details", new { id = item.idFornecedor }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.idFornecedor })
            </td>
        </tr>
    }

</table>
