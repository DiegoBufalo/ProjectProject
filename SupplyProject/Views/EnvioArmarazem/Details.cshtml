@model SupplyProject.Models.EnvioArmarazem

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Detalhes</h2>

<div>
    <h4>Envios do Armarazem</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Id Envio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.idEnvio)
        </dd>

        <dt>
            Id Demanda
        </dt>

        <dd>
            @Html.DisplayFor(model => model.idDemanda)
        </dd>

        <dt>
            Placa do veiculo transportador
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Veiculo.placa_veiculo)
        </dd>

        <dt>
            Status Envio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StatusEnvioArmazem.statusEnvio)
        </dd>

        <dt>
            Data de envio
        </dt>

        <dd>
            @Html.DisplayFor(model => model.dia_envio)/@Html.DisplayFor(model => model.mes_envio)/@Html.DisplayFor(model => model.ano_envio)
        </dd>

    </dl>
</div>
<p>
    
    @Html.ActionLink("Voltar para lista", "Index")
</p>
